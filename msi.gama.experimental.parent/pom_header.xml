	<repositories>
		<repository>
			<id>p2Repo</id>
			<name>p2Repo</name>
			<url>https://updates.gama-platform.org/${gama.version}</url>
			<layout>p2</layout>
			<snapshots>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</snapshots>
			<releases>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</releases>
		</repository>
		<repository>
			<id>gamaRepo</id>
			<name>gamaRepo</name>
			<url>https://updates.gama-platform.org/${gama.version}</url>
			<layout>default</layout>
			<snapshots>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</snapshots>
			<releases>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</releases>
		</repository>
		<repository>
			<id>eclipse-swt</id>
			<url>https://download.eclipse.org/releases/2022-12</url>
			<layout>p2</layout>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>

